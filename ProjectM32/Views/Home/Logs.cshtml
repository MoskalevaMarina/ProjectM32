@{
    ViewData["Title"] = "История запросов к проекту";
}


<div class="text-center">
    <h2 class="display-4" style="text-align: left">История запросов к проекту </h2>
    <br>
    <table class="table table-bordered">
        <thead class="table-dark">
            <tr>
                <th>Идентификатор</th>
                <th>Дата</th>
                <th>Url</th>
            </tr>
            </thead>
            @foreach (var item in Model)
            {
                <tr>
                    <td>@item.Id</td>
                    <td>@item.Date</td>
                    <td>@item.Url</td>
                </tr>
            }
    </table>
</div>